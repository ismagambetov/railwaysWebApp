<restriction>
    <!--an admin role does not have any restrictions-->
    <admin>
        <page>*</page>
    </admin>

    <!--a cashier role has some pages, where an access is restricted-->
    <cashier>
        <page>admin-page.jsp</page>
    </cashier>

    <!--a passenger role has some pages, where an access is restricted-->
    <passenger>
        <page>admin-page.jsp</page>
    </passenger>

</restriction>